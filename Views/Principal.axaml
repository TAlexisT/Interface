				<Button Click="GrafoB" Grid.Column="1" Height="40" Width="190" HorizontalContentAlignment="Center" Background="YellowGreen" HorizontalAlignment="Center" VerticalContentAlignment="Center" FontWeight="SemiBold" VerticalAlignment="Center" >Graph</Button>
			</Grid>
</Window>
